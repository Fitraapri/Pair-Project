<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <h1><a href="/">Home</a></h1>
  <a href="/register">Register</a>
  <a href="/login">Login</a>

  <table border="1">
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Image</th>
      <th>Action</th>
    </tr>
    <%Item.forEach(el=>{%>
      <tr>
        <td>
          <%=el.name%>
        </td>
        <td>
          <%=el.description%>
        </td>
        <td>
          <%=el.price%>
        </td>
        <td>
          <img src="<%= el.imageUrl %>" alt="" height="50" width="50">

        </td>
        <td>
          <button>Add</button>
        </td>
      </tr>
      <%})%>
  </table>

</body>

</html>